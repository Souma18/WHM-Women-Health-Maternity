package datas;

public class TaiKhoan {
private int id;
private String tenTaiKhoan;
private String matKhau;
private int role;
public TaiKhoan(int id, String tenTaiKhoan, String matKhau, int role) {
	super();
	this.id = id;
	this.tenTaiKhoan = tenTaiKhoan;
	this.matKhau = matKhau;
	this.role = role;
}

}
